<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>ApiCommon.Extensions.Newtonsoft</AssemblyName>
    <PackageId>ApiCommon.Extensions.Newtonsoft</PackageId>
    <Authors>Bradley Clarke</Authors>
    <Company>Maritime Superanutation</Company>
    <Product>ApiCommon</Product>
    <Version>1.2.1.3</Version>
    <PackageLicenseFile></PackageLicenseFile>
    <PackageIcon></PackageIcon>
    <RepositoryUrl></RepositoryUrl>
    <RepositoryType></RepositoryType>
    <PackageTags></PackageTags>
    <NeutralLanguage>en-AU</NeutralLanguage>
    <Copyright></Copyright>
    <Description></Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <IncludeSymbols>true</IncludeSymbols>
      <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ApiCommon.Abstractions\ApiCommon.Abstractions.csproj" />
  </ItemGroup>

</Project>
